<h1> Wikis </h1><%= link_to "New Wiki", new_wiki_path %>
<% @wikis.each do |wiki | %>
<% if wiki.private %>
<% if current_user.admin? || current_user.premium? %>
<h3> * Private Post * <%= link_to wiki.title, wiki_path(wiki.id) %> by<small><%= wiki.user.email %></small></h3>
<hr/>
<% end %>
<% else %>
<h3><%= link_to wiki.title, wiki_path(wiki.id) %> by<small><%= wiki.user.email %></small></h3>
<hr/>
<% end %>

<% end %>
